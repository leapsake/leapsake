{
  "name": "@leapsake/server",
  "version": "0.0.0",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.app.json",
    "dev": "tsc -p tsconfig.app.json && node dist/main.js",
    "start": "node dist/main.js",
    "lint": "eslint src --ext .ts",
    "db:generate": "drizzle-kit generate --name",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "express": "^4.21.2",
    "multer": "^2.0.0",
    "pg": "^8.16.0",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "~18.16.9",
    "@types/pg": "^8.15.4",
    "eslint": "^9.8.0",
    "typescript": "^5.7.2"
  }
}
